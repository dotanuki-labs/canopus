{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "codeowners",
      "camelcase": "Codeowners",
      "title": "Codeowners",
      "scope": "source.codeowners",
      "file-types": [
        "codeowners"
      ],
      "injection-regex": "^codeowners$",
      "class-name": "TreeSitterCodeowners"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Codeowners grammar for tree-sitter",
    "authors": [
      {
        "name": "Ubiratan Soares",
        "email": "ubiratan.f.soares@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-codeowners"
    }
  },
  "bindings": {
    "c": false,
    "go": false,
    "node": false,
    "python": false,
    "rust": true,
    "swift": false,
    "zig": false
  }
}
